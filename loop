#include <iostream>
#include <cmath>
using namespace std;

int main(){
    for(int i = 0; i < 6; ++i){
        for(int j = 0; j < 2; ++j){
            if(j == 1){
                if(i > 1) cout << pow(2, i*2) << " ";
                else cout << pow(2, i + 1) << " ";
            }else
            cout << pow(2, i) << " ";
        }
        cout << endl;
    }

    return 0;
}
