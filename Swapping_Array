#include <iostream>

using namespace std;

int main()
{
    int A[10], B[10], temp, *aPtr, *bPtr;

    cout << "A matrix: \n";
    for ( int i = 0; i < 10; ++i ){ cout << "Enter Elements[" << i + 1 << "]: "; cin >> A[i]; }

    cout << "\n\nB matrix: \n";
    for ( int i = 0; i < 10; ++i ){ cout << "Enter Elements[" << i + 1 << "]: "; cin >> B[i]; }

    cout << "\nDisplay A matrix: \n";
    for ( int i = 0; i < 10; ++i ){ cout << A[i] << " "; }
    cout << "\n\nDisplay B matrix: \n";
    for ( int i = 0; i < 10; ++i ){ cout << B[i] << " "; }

    aPtr = A;
    bPtr = B;
    for ( int i = 0; i < 10; ++i ){
        temp = *(aPtr + i);
        *(aPtr + i) = *(bPtr + i);
        *(bPtr + i) = temp;
    }

    cout << "\n\nAfter Swapping: \n";
    cout << "\nA matrix: \n";
    for ( int i = 0; i < 10; ++i ){ cout << A[i] << " "; }

    cout << "\n\nB matrix: \n";
    for ( int i = 0; i < 10; ++i ){ cout << B[i] << " "; }
    cout << "\n";

    return 0;
}

