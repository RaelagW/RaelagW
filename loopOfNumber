#include <iostream>
using namespace std;
#define PW 1999
int main(){
  int x;
  cout << "Enter number less than 1000 to break the loop: \n\n";
  for ( int i = 0; i < 5; ++i ){
    cout << "Enter integer number between 1000 & 9999: ";
    cin >> x;
    if ( x < 1000 || x > 9999 ) { cout << "Invalid range\n"; break; }
    if ( x == PW ) cout << "Correct\n";
    else cout << "Wrong\n";
  }
  return 0;
}
