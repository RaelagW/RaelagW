#include <iostream>

using namespace std;

int main(){
    cout << "Enter number of rows: ";
    int r;
    cin >> r;
    cout << "Enter number of columns: ";
    int c;
    cin >> c;
    int i, j;
    i = j = 1;
    cout << endl;
    while(i <= r){
        while(j <= c){
            cout << "* ";
            ++j;
        }
        cout << endl;
        ++i;
        j = 1;
    }
    return 0;
}
