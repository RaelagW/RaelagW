#include <iostream>
#include <cmath>

using namespace std;

int main(){
    int x;
    cout << "Enter integer number: ";
    cin >> x;
    int n;
    cout << "Enter range: ";
    cin >> n;
    int sum = 0;
    for(int i = 1; i <= n; ++i){
        sum += pow(x, i/i);
    }
    cout << "Sum = " << sum;
    return 0;
}
