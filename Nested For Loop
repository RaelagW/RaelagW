#include <iostream>
using namespace std;

int main() {
  int k = 4;
  for(int i = 1; i<=7; ++i){
    for(int j = 1; j<=7; ++j){
      if(i==1)cout << 4 << " ";
      else if(j==1) cout << 4 << " ";
      else if(i == 7) cout << 4 << " ";
      else if(j == 7) cout << 4 << " ";
      else if(i==2)cout << 3 << " ";
      else if(j==2) cout << 3 << " ";
      else if(i == 6) cout << 3 << " ";
      else if(j == 6) cout << 3 << " ";
      else if(i==3)cout << 2 << " ";
      else if(j==3) cout << 2 << " ";
      else if(i == 5) cout << 2 << " ";
      else if(j == 5) cout << 2 << " ";
      else if(i ==4 && j ==4 )cout << 1 << " ";
      else cout << "  ";
    }
    cout << endl;
  }
}
