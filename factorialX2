#include <iostream>
#include <cmath>
using namespace std;

int main(){
    int sum = 0;
    int fact = 1;
    for(int i = 1; i <= 10; ++i){
        int temp = i;
        cout << i << " != ";
        while( i > 0){
            if( i == 1 ) cout << i << " = " << fact << endl;
            else cout << i << " * ";
            fact *= i;
            sum += fact;
            --i;
        }
        fact = 1;
        i = temp;
    }

    return 0;
}
