#include <iostream>

using namespace std;

int main()
{
    string str;
    cout << "Enter string: ";
    getline(cin, str);
    int counter = 0;
    for ( int i = 0; i < str.length(); ++i ) if ( str[i] == 'a' || str[i] == 'o' || str[i] == 'u' || str[i] == 'i'|| str[i] == 'e' ) ++counter;
    cout << str << " contain " << counter << " of ( a, o, u, e, i )" << endl;
    return 0;
}
